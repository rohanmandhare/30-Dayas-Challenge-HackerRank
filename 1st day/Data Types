import java.util.*;
import java.util.Scanner;

public class solution{
public static void main(String[] args){
  Scanner input = new Scanner(System.in);
  int i = 12;
  double d = 4.0;
  String s = "HackerRank ";
  int a;
  double b;
  String s1;
  a = input.nextInt();
  b = input.nextDouble();
  input.nextLine();
  s1 = input.nextLine();
  
  System.out.println(i + a);
  System.out.println(d + b);
  System.out.println(s.concat(s1));
  input.close();
  
 }
}
