import java.io.*;
import java.util.Scanner;
import java.util.*;

public class Solution{
public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int T=sc.nextInt();
    sc.nextLine();  //to clear buffer
    for(int i=0;i<=T-1;i++){
        String S=sc.nextLine();
        for(int k=0;k<=1;k++){
            for(int j=k;j<S.length();j=j+2){
                System.out.print(S.charAt(j));
            }
            System.out.print(" ");  //to add space between even and odd chars
        }
        System.out.println(); //to enter new line for second string 
    }

}  
}
